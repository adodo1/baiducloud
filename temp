except:
					try:
						utils.async_call(down_img, thumbimg,thumb,fsid,header,path,
                         callback=create_img_pix)
						#down_img(thumbimg,thumb,fsid,header,path)
						pix = create_img_pix(thumbimg)
					except:
						pix = None

				#logger.debug("Thumbs link: %s"%thumb)
				host = urlparse(thumb)[1]
				header = {
				'Host':host,
				'Referer':DISK_HOME,
				}
				if not os.path.exists("image"):
						os.makedirs("image")
				thumbimg = "image/" + fsid + ".jpg"
				if not settings.DRY and not os.path.exists(thumbimg) :
					utils.async_call(down_img, thumbimg,thumb,fsid,header,path,
                         callback=create_img_pix)
					#down_img(thumbimg,thumb,fsid,header,path)
				#try:
				pix = create_img_pix(thumbimg)
				
			elif category == 6:
				pix = create_img_pix("image/folder.png")
